<%= semantic_form_for @team_payment do |f| %>
  <%= f.inputs do %>
    <%= f.input :status %>
    <%= f.input :amount %>
    <%= f.input :email %>
    <%= f.input :transaction %>
    <%= f.input :stripe_customer_token %>
    <%= f.input :team %>
    <%= f.input :memberships_purchased %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
