            <a class="manta" href="http://www.manta.com/cpl-edit/mxdhmsq#widget:recommendations;w:250;h:470;emid:mxdhmsq"><img src="assets/website/icons/social/manta.png" alt="Manta" class="footer-social-icon hover-fade"></a> -->
        </div>
        <p class="breadcrumb"><a href="/home">Home</a><span class='current_crumb'>Contact Us</span></p>
    </header>
    <!-- end header -->

    <%= render 'main_menu', a: {'contact_us' => 'active'} %>

    <div id="container">

        <div class="fullwidth-heading">
            <h2>Connect with us</h2>
        </div>
        <!-- end fullwidth-heading -->
        <br class="clear-space">

        <!--  START FLIGHT  -->
        <div class="one-half">
            <%= form_tag('/email', :id => 'contact_form', :autocomplete => 'off' ) do %>
                <%= text_field :email, :name, :id => "contact-name",  :autocomplete => 'none', :class => "contact-name", :placeholder => "Name", :onfocus => "if(this.value==this.defaultValue)this.value='';", :onblur => "if(this.value=='')this.value=this.defaultValue;" %> 
                <%= text_field :email, :address, :id => "contact-email", :class => "contact-email", :placeholder => "Email", :onfocus => "if(this.value==this.defaultValue)this.value='';", :onblur => "if(this.value=='')this.value=this.defaultValue;" %>
                <%= text_field :email, :website, :id => "contact-website", :class => "contact-website", :placeholder => "Website", :onfocus => "if(this.value==this.defaultValue)this.value='';", :onblur => "if(this.value=='')this.value=this.defaultValue;" %>
                <%= text_field :email, :subject, :id => "contact-subject", :class => "contact-subject", :placeholder => "Subject", :onfocus => "if(this.value==this.defaultValue)this.value='';", :onblur => "if(this.value=='')this.value=this.defaultValue;" %>
                <%= text_area :email, :body, :rows => "6", :cols => "25", :id => "contact-message", :class => "contact-message", :placeholder => "Message" %>
                <br><br>
                <%= submit_tag "Send", :id => "contact-submit", :value => "Send Request", :class => "medium button contact-submit" %>
            <% end %>
        </div>
        <!--  END FLIGHT  -->


        <article class="one-third">
            <h3>Contact info</h3>
            <!-- end contact-info -->   
            <section class="contact-numbers">
                <%# <b>Toll-Free:</b>1-866-849-8081
                <br> %>

                <b>Email:</b> <a href="mailto:info@idigitallsports.com">info@idigitallsports.com</a>
            </section>
            <!-- end contact-numbers -->
        </article>
        <br class="clear-space">

